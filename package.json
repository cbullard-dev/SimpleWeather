{
  "name": "simpleweather",
  "packageManager": "yarn@3.2.1",
  "description": "My own simple weather app in Node.JS",
  "version": "0.0.1",
  "main": "/src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -r dotenv/config src/app.ts",
    "build": "tsc -p",
    "type-coverage": "type-coverage"
  },
  "author": "Callum Bullard",
  "license": "MIT",
  "keywords": [
    "Weather",
    "Node",
    "Express",
    "Axios"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "slack-block-builder": "^2.6.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.16",
    "ts-node-dev": "^2.0.0",
    "type-coverage": "^2.22.0",
    "typescript": "^4.7.4"
  }
