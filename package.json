{
  "name": "simpleweather",
  "packageManager": "yarn@3.2.1",
  "description": "A simple Slack weather app built in TypeScript",
  "version": "0.0.1",
  "main": "/src/app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -r dotenv/config src/app.ts",
    "build": "tsc -p",
    "type-coverage": "type-coverage",
    "lint": "eslint --ext .js,.ts ."
  },
  "author": "Cal Bullard",
  "license": "MIT",
  "keywords": [
    "Weather",
    "Node",
    "Express",
    "Axios"
  ],
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "slack-block-builder": "^2.6.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.16",
    "ts-node-dev": "^2.0.0",
    "type-coverage": "^2.22.0",
    "typescript": "^4.9.4"
  }
}
